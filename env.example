# ============================================================================
# AGORA CONFIGURATION (Required)
# ============================================================================
# Get these from https://console.agora.io
AGORA_APP_ID=your_app_id_here
AGORA_APP_CERTIFICATE=your_app_certificate_here

# Agora REST API Credentials (for ConvoAI and Channel Management)
# Get these from Agora Console > Projects > ConversationalAI
AGORA_CUSTOMER_ID=your_customer_id_here
AGORA_CUSTOMER_SECRET=your_customer_secret_here

# ============================================================================
# GAME CONFIGURATION (Optional - has defaults)
# ============================================================================
BINGO_BOARD_SIZE=5
BINGO_TOKEN_VALIDITY_SECONDS=3600
BINGO_TOKEN_RENEWAL_THRESHOLD_SECONDS=300
CONVOAI_API_URL=https://api.agora.io/api/conversational-ai-agent/v2

# ============================================================================
# LLM CONFIGURATION (Choose ONE vendor by uncommenting)
# ============================================================================

# --- OpenAI (Default) ---
LLM_VENDOR=openai
LLM_URL=https://api.openai.com/v1/chat/completions
LLM_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4o-mini
# LLM_BASE_URL=  # Optional: for OpenAI-compatible APIs
# LLM_ACCESS_KEY=  # Optional: for some providers
# LLM_SECRET=  # Optional: for some providers

# --- Azure OpenAI ---
# LLM_VENDOR=azure_openai
# LLM_URL=https://your-resource.openai.azure.com/openai/deployments/your-deployment/chat/completions?api-version=2024-02-15-preview
# LLM_API_KEY=your_azure_openai_key
# LLM_MODEL=gpt-4

# --- Anthropic Claude ---
